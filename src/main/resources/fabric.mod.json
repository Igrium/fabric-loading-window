{
	"schemaVersion": 1,
	"id": "loading-window",
	"version": "${version}",
	"name": "Loading Window",
	"description": "A dead-simple mod that adds an AWT loading window before Minecraft's main window opens.",
	"authors": [
		"Igrium"
	],
	"contact": {
		"homepage": "https://github.com/Igrium/fabric-loading-window",
		"sources": "https://github.com/Igrium/fabric-loading-window"
	},
	"license": "MIT",
	"icon": "assets/loading-window/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.igrium.loading_window.Initializer"
		],
		"preLaunch": [
			"com.igrium.loading_window.PreLaunch"
		]
	},
	"mixins": [
		"loading-window.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.0",
		"minecraft": ">=1.18"
	},
	"suggests": {
		"another-mod": "*"
	}
}